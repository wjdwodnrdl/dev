[32m+[m[32m            matchLabels:[m
[32m+[m[32m              app: $_PROJECT_NAME[m
[32m+[m[32m          template:[m
[32m+[m[32m            metadata:[m
[32m+[m[32m              labels:[m
[32m+[m[32m                app: $_PROJECT_NAME[m
[32m+[m[32m            spec:[m
